    "type": "library",
    "require": {
        "illuminate/support": "5.8.*",
        "illuminate/console": "5.8.*"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Ryan Grissinger",
            "email": "ryan.grissinger@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
